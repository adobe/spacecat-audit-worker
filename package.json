{
  "name": "@adobe/spacecat-audit-worker",
  "version": "1.276.0",
  "description": "SpaceCat Audit Worker",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=24.0.0 <25.0.0",
    "npm": ">=10.9.0"
  },
  "scripts": {
    "start": "nodemon",
    "test": "c8 mocha -i -g 'Post-Deploy' --spec=test/**/*.test.js",
    "test:spec": "c8 mocha -i -g 'Post-Deploy'",
    "test:bundle": "HELIX_TEST_BUNDLE_NAME=dist/spacecat-services/audit-worker@$(npm pkg get version | tr -d \\\")-bundle.mjs mocha --spec=test/index.test.js",
    "test-postdeploy": "mocha -g 'Post-Deploy' --spec=test/**/*.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "logs": "aws logs tail /aws/lambda/spacecat-services--audit-worker",
    "semantic-release": "semantic-release",
    "semantic-release-dry": "semantic-release --dry-run --no-ci --branches $CI_BRANCH",
    "build": "hedy -v --test-bundle",
    "deploy": "hedy -v --deploy --aws-deploy-bucket=spacecat-prod-deploy --pkgVersion=latest",
    "deploy-stage": "hedy -v --deploy --aws-deploy-bucket=spacecat-stage-deploy --pkgVersion=latest",
    "deploy-dev": "hedy -v --deploy --pkgVersion=ci$CI_BUILD_NUM -l latest --aws-deploy-bucket=spacecat-dev-deploy --cleanup-ci=24h",
    "deploy-secrets": "hedy --aws-update-secrets --params-file=secrets/secrets.env",
    "prepare": "husky",
    "local-build": "sam build",
    "local-run": "sam local invoke -l output.txt",
    "local-build-run": "sam build && sam local invoke -l output.txt",
    "local-watch": "node scripts/watch-and-copy.js"
  },
  "hlx": {
    "target": "aws",
    "linkRoutes": false,
    "name": "spacecat-services/audit-worker@${version}",
    "memory": 6144,
    "awsRole!important": "arn:aws:iam::${env.AWS_ACCOUNT_ID}:role/spacecat-role-lambda-generic",
    "testUrl": "/_status_check/healthcheck.json",
    "awsAttachAuthorizer": "",
    "dev": {
      "params-file": "secrets/dev-secrets.json"
    },
    "fastlyServiceId!important": "",
    "timeout": 900000,
    "nodeVersion": 24,
    "static": [
      "static/prompts/broken-backlinks.prompt",
      "static/prompts/broken-backlinks-followup.prompt",
      "static/prompts/heading-empty-suggestion.prompt",
      "static/prompts/toc-detection.prompt",
      "static/prompts/generate-brand-guidelines.prompt",
      "static/schemaorg-current-https.jsonld",
      "src/cdn-analysis/sql",
      "src/cdn-logs-report/sql",
      "src/llmo-referral-traffic/sql",
      "src/page-intent/sql",
      "src/llm-error-pages/sql",
      "src/llmo-customer-analysis/sql",
      "src/page-citability/sql"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/spacecat-audit-worker"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/spacecat-audit-worker/issues"
  },
  "homepage": "https://github.com/adobe/spacecat-audit-worker#readme",
  "mocha": {
    "require": "test/setup-env.js",
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "dependencies": {
    "@adobe/fetch": "4.2.3",
    "@adobe/helix-shared-secrets": "2.3.0",
    "@adobe/helix-shared-wrap": "2.0.2",
    "@adobe/helix-status": "10.1.5",
    "@adobe/helix-universal-logger": "3.0.28",
    "@adobe/spacecat-helix-content-sdk": "1.4.33",
    "@adobe/spacecat-shared-ahrefs-client": "1.10.3",
    "@adobe/spacecat-shared-athena-client": "1.9.1",
    "@adobe/spacecat-shared-data-access": "2.92.0",
    "@adobe/spacecat-shared-google-client": "1.4.62",
    "@adobe/spacecat-shared-gpt-client": "1.6.15",
    "@adobe/spacecat-shared-html-analyzer": "1.2.0",
    "@adobe/spacecat-shared-http-utils": "1.19.4",
    "@adobe/spacecat-shared-ims-client": "1.11.7",
    "@adobe/spacecat-shared-rum-api-client": "2.40.3",
    "@adobe/spacecat-shared-scrape-client": "2.3.6",
    "@adobe/spacecat-shared-slack-client": "1.5.32",
    "@adobe/spacecat-shared-tier-client": "1.3.10",
    "@adobe/spacecat-shared-utils": "https://gist.github.com/tkotthakota-adobe/0bcfeb9e5daac09bb328ae94bc9dfdd7/raw/b63b067b1b5b516b65784280aa6770290626f974/adobe-spacecat-shared-utils-1.86.0.tgz",
    "@adobe/structured-data-validator": "1.5.0",
    "@aws-sdk/client-athena": "3.940.0",
    "@aws-sdk/client-lambda": "3.940.0",
    "@aws-sdk/client-s3": "3.940.0",
    "@aws-sdk/client-sqs": "3.940.0",
    "@aws-sdk/credential-provider-node": "3.940.0",
    "@aws-sdk/s3-request-presigner": "3.940.0",
    "aws-xray-sdk": "3.12.0",
    "cheerio": "1.1.2",
    "date-fns": "4.1.0",
    "diff": "8.0.2",
    "exceljs": "4.4.0",
    "franc-min": "6.2.0",
    "get-xpath": "3.3.0",
    "hyparquet": "1.22.1",
    "hyparquet-writer": "0.10.0",
    "hyphen": "1.10.6",
    "is-language-code": "5.1.0",
    "js-beautify": "1.15.4",
    "robots-parser": "3.0.1",
    "syllable": "5.0.1",
    "text-readability": "1.1.1",
    "urijs": "1.19.11",
    "zod": "4.1.13",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "3.0.15",
    "@adobe/helix-deploy": "13.2.2",
    "@adobe/helix-universal": "5.3.0",
    "@adobe/helix-universal-devserver": "1.1.143",
    "@adobe/semantic-release-coralogix": "1.1.40",
    "@adobe/semantic-release-skms-cmr": "1.1.5",
    "@babel/core": "7.28.5",
    "@babel/eslint-parser": "7.28.5",
    "@babel/plugin-syntax-import-assertions": "7.27.1",
    "@eslint/config-helpers": "0.5.0",
    "@redocly/cli": "2.12.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "13.1.2",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "c8": "10.1.3",
    "chai": "6.2.1",
    "chai-as-promised": "8.0.2",
    "chokidar": "4.0.3",
    "dotenv": "17.2.3",
    "eslint": "9.39.1",
    "esmock": "2.7.3",
    "husky": "9.1.7",
    "junit-report-builder": "5.1.1",
    "lint-staged": "16.2.7",
    "mocha": "11.7.5",
    "mocha-multi-reporters": "1.5.1",
    "mocha-suppress-logs": "0.6.0",
    "nock": "14.0.10",
    "nodemon": "3.1.11",
    "semantic-release": "25.0.2",
    "sinon": "21.0.0",
    "sinon-chai": "4.0.1",
    "typescript": "5.9.3",
    "yaml": "2.8.1"
  },
  "overrides": {
    "@adobe/fetch": "4.2.3",
    "@adobe/spacecat-shared-data-access": "2.92.0"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.ts": "eslint",
    "*.cjs": "eslint"
  },
  "nodemonConfig": {
    "exec": "node --inspect ./test/dev/server.mjs",
    "watch": [
      ".env",
      "."
    ],
    "ext": ".js, .cjs, .ejs, .css"
  }
}
